# ðŸ§¬ Lilith.Eve Environment Configuration
# Copy this file to .env and configure your environment variables

# ============================================================================
# APPLICATION CONFIGURATION
# ============================================================================
NODE_ENV=development
APP_PORT=3000
APP_HOST=localhost
APP_URL=http://localhost:3000
BUILD_TARGET=development

# ============================================================================
# DATABASE CONFIGURATION
# ============================================================================
DATABASE_URL=postgresql://lilith:your_password@localhost:5432/lilith_eve
POSTGRES_DB=lilith_eve
POSTGRES_USER=lilith
POSTGRES_PASSWORD=your_secure_password
POSTGRES_PORT=5432

# ============================================================================
# REDIS CONFIGURATION
# ============================================================================
REDIS_URL=redis://:your_redis_password@localhost:6379
REDIS_PASSWORD=your_redis_password
REDIS_PORT=6379

# ============================================================================
# ELASTICSEARCH CONFIGURATION
# ============================================================================
ELASTICSEARCH_URL=http://localhost:9200
ELASTICSEARCH_PORT=9200
KIBANA_PORT=5601

# ============================================================================
# MESSAGE QUEUE CONFIGURATION
# ============================================================================
RABBITMQ_URL=amqp://lilith:your_rabbitmq_password@localhost:5672
RABBITMQ_USER=lilith
RABBITMQ_PASSWORD=your_rabbitmq_password
RABBITMQ_PORT=5672
RABBITMQ_MANAGEMENT_PORT=15672

# ============================================================================
# AI/ML API KEYS
# ============================================================================
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key
PINECONE_API_KEY=your_pinecone_api_key
PINECONE_ENVIRONMENT=your_pinecone_environment
COHERE_API_KEY=your_cohere_api_key
HUGGINGFACE_API_KEY=your_huggingface_api_key

# ============================================================================
# AUTHENTICATION & SECURITY
# ============================================================================
JWT_SECRET=your_super_secure_jwt_secret_key_here
JWT_EXPIRES_IN=24h
JWT_REFRESH_EXPIRES_IN=7d
BCRYPT_ROUNDS=12
SESSION_SECRET=your_session_secret_key

# ============================================================================
# VAULT CONFIGURATION
# ============================================================================
VAULT_URL=http://localhost:8200
VAULT_ROOT_TOKEN=your_vault_root_token
VAULT_PORT=8200

# ============================================================================
# MONITORING & OBSERVABILITY
# ============================================================================
PROMETHEUS_PORT=9090
GRAFANA_PORT=3001
GRAFANA_PASSWORD=your_grafana_password
NEW_RELIC_LICENSE_KEY=your_new_relic_license_key
NEW_RELIC_APP_NAME=Lilith.Eve

# ============================================================================
# EXTERNAL API INTEGRATIONS
# ============================================================================
# Social Media APIs (with consent)
TWITTER_API_KEY=your_twitter_api_key
TWITTER_API_SECRET=your_twitter_api_secret
TWITTER_ACCESS_TOKEN=your_twitter_access_token
TWITTER_ACCESS_SECRET=your_twitter_access_secret

INSTAGRAM_ACCESS_TOKEN=your_instagram_access_token
FACEBOOK_APP_ID=your_facebook_app_id
FACEBOOK_APP_SECRET=your_facebook_app_secret

# Wearable Device APIs
FITBIT_CLIENT_ID=your_fitbit_client_id
FITBIT_CLIENT_SECRET=your_fitbit_client_secret
APPLE_HEALTH_KIT_ENABLED=true
GARMIN_API_KEY=your_garmin_api_key

# Medical Device APIs
ECG_DEVICE_API_KEY=your_ecg_device_api_key
EEG_DEVICE_API_KEY=your_eeg_device_api_key
BLOOD_PRESSURE_API_KEY=your_blood_pressure_api_key

# ============================================================================
# TRANSLATION & LOCALIZATION
# ============================================================================
DEEPL_API_KEY=your_deepl_api_key
GOOGLE_TRANSLATE_API_KEY=your_google_translate_api_key
SUPPORTED_LANGUAGES=en,es,fr,de,zh,ja,ko,ar,hi

# ============================================================================
# STORAGE & FILE UPLOADS
# ============================================================================
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-west-2
AWS_S3_BUCKET=lilith-eve-storage
AWS_S3_ENCRYPTION_KEY=your_s3_encryption_key

# ============================================================================
# EMAIL & NOTIFICATIONS
# ============================================================================
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_email_password
SMTP_SECURE=true

SENDGRID_API_KEY=your_sendgrid_api_key
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# ============================================================================
# PAYMENT PROCESSING
# ============================================================================
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# ============================================================================
# DEVELOPMENT TOOLS
# ============================================================================
ADMINER_PORT=8080
LOG_LEVEL=debug
ENABLE_SWAGGER=true
ENABLE_GRAPHIQL=true

# ============================================================================
# FEATURE FLAGS
# ============================================================================
ENABLE_BIOMETRIC_ANALYSIS=true
ENABLE_SOCIAL_MEDIA_ANALYSIS=true
ENABLE_HOLISTIC_TREATMENTS=true
ENABLE_CULTURAL_INTEGRATION=true
ENABLE_EMERGENCY_DETECTION=true
ENABLE_DREAM_ANALYSIS=false

# ============================================================================
# PRIVACY & COMPLIANCE
# ============================================================================
GDPR_COMPLIANCE_ENABLED=true
HIPAA_COMPLIANCE_ENABLED=true
DATA_RETENTION_DAYS=2555
AUDIT_LOGGING_ENABLED=true
ENCRYPTION_AT_REST=true
ENCRYPTION_IN_TRANSIT=true

# ============================================================================
# PERFORMANCE & SCALING
# ============================================================================
MAX_CONCURRENT_ANALYSES=100
ANALYSIS_TIMEOUT_MS=300000
CACHE_TTL_SECONDS=3600
RATE_LIMIT_REQUESTS_PER_MINUTE=100
RATE_LIMIT_BURST_SIZE=20

# ============================================================================
# CULTURAL & HEALING CONFIGURATION
# ============================================================================
DEFAULT_CULTURAL_SENSITIVITY=high
ENABLE_TRADITIONAL_MEDICINE=true
ENABLE_ENERGY_HEALING=true
ENABLE_SPIRITUAL_INTEGRATION=true
CULTURAL_DATABASE_URL=your_cultural_database_url

# ============================================================================
# TESTING CONFIGURATION
# ============================================================================
TEST_DATABASE_URL=postgresql://lilith_test:test_password@localhost:5433/lilith_eve_test
TEST_REDIS_URL=redis://localhost:6380
JEST_TIMEOUT=30000
COVERAGE_THRESHOLD=80 